# Copy this file to .env and fill in your Nixbit credentials

# Required: Get these from your Nixbit dashboard
NIXBIT_API_KEY=your-api-key-here
NIXBIT_PROJECT_ID=your-project-id-here

# Optional: Override default API URL
NIXBIT_API_URL=https://nixbit.dev/api

# Optional: Enable debug logging
NIXBIT_DEBUG=true

# These are typically auto-detected in CI environments
# CI_COMMIT_REF_NAME=main
# CI_COMMIT_SHA=abc123def456
# CI_PIPELINE_ID=12345